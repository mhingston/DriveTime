<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7"/>
  </startup>
  <connectionStrings xdt:Transform="Replace">
    <add name="Test" connectionString="Data Source=sa;Persist Security Info=True;User ID=sa;Password=sa;Database=Test;" providerName="System.Data.SqlClient"/>
  </connectionStrings>
  <appSettings xdt:Transform="Replace">
    <add key="DelayBetweenRequestsMs" value="333"/>
    <add key="SleepDurationMs" value="300000"/> <!-- 5 minutes -->
    <add key="ApiKey" value="" />
  </appSettings>
</configuration>
